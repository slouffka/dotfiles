#  Images
image/jpeg; /usr/bin/feh -Z -x --fontpath /home/alex/.fonts/ -M calibri/10 -N -B black -e calibri/10 %s; test=test -n "$DISPLAY"
image/png; /usr/bin/feh -Z -x --fontpath /home/alex/.fonts/ -M calibri/10 -N -B black -e calibri/10 %s; test=test -n "$DISPLAY"
image/gif; /usr/bin/feh -Z -x --fontpath /home/alex/.fonts/ -M calibri/10 -N -B black -e calibri/10 %s; test=test -n "$DISPLAY"

#  PDFs
application/pdf; /usr/bin/zathura %s; test=test -n "$DISPLAY"
